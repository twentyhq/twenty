{
  "name": "twenty-website",
  "private": true,
  "scripts": {
    "nx": "NX_DEFAULT_PROJECT=twenty-website node ../../node_modules/nx/bin/nx.js",
    "dev": "npx next dev",
    "build": "npx next build",
    "start": "npx next start",
    "lint": "ESLINT_USE_FLAT_CONFIG=true npx next lint",
    "github:sync": "npx tsx src/github/github-sync.ts",
