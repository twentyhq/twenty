{
  "name": "create-twenty-app",
  "version": "0.2.4",
  "description": "Command-line interface to create Twenty application",
  "main": "dist/cli.cjs",
  "bin": "dist/cli.cjs",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "npx rimraf dist && npx vite build"
  },
  "keywords": [
    "twenty",
    "cli",
